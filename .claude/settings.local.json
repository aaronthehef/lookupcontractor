{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(node:*)",
      "Bash(ping:*)",
      "Bash(curl:*)",
      "Bash(nslookup:*)",
      "Bash(ipconfig /flushdns)",
      "Bash(powershell:*)",
      "Bash(npm install:*)",
      "Bash(sed:*)",
      "Bash(grep:*)",
      "Bash(vercel:*)",
      "Bash(mkdir:*)",
      "Bash(npm run build:*)",
      "Bash(rm -rf .vercel .next node_modules/.cache)",
      "Bash(rm vercel.json)",
      "Bash(rm -rf app)",
      "Bash(rm -rf .next)",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(git reset:*)",
      "Bash(git commit:*)",
      "Bash(git config:*)",
      "Bash(git remote add:*)",
      "Bash(git branch:*)",
      "Bash(git push:*)",
      "Bash(pkill:*)",
      "Bash(taskkill:*)",
      "Bash(move:*)",
      "Bash(mv:*)",
      "Bash(cmd /c:*)",
      "Bash(tasklist:*)",
      "Bash(find:*)",
      "Bash(wmic process where processid=23780 delete:*)",
      "Bash(rm:*)",
      "WebFetch(domain:www.contractorslicenseguru.com)",
      "WebFetch(domain:www.gravel2gavel.com)",
      "WebFetch(domain:www.cslb.ca.gov)",
      "Bash(npm run dev:*)",
      "Bash(set PORT=3011)",
      "Bash(for i in {2..25})",
      "Bash(do sed -i \"s/limit = 10000/limit = 5000/g\" sitemap-contractors-$i.xml.js)",
      "Bash(done)",
      "Bash(for i in {26..50})",
      "Bash(do cp sitemap-contractors-25.xml.js sitemap-contractors-$i.xml.js)",
      "Bash(ls:*)"
    ],
    "deny": []
  }
}